node {    
    stage('Run playbook'){
        dir('/etc/ansible/') {
          ansiblePlaybook  ([
                inventory: 'hosts',
                playbook    : 'test1.yml',
                hosts: "server2",
                  extraVars: [
                        servicename: "${servicename}",
                        replica: "${replica}",
                        functions: "${functions}"
            ],
         ])
        }
    }
}
